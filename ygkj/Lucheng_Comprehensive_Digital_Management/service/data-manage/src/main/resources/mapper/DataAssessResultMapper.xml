<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcy.datamanage.biz.mapper.DataAssessResultMapper">

    <insert id="replaceAll" parameterType="com.dcy.datamanage.api.model.DataAssessExtendMsg">
        REPLACE INTO data_assess_result (id,data_source_id,`table_name`,score,update_time)
        VALUE
            <foreach collection="list" item="item" separator=",">
                (#{item.id},#{item.dataSourceId},#{item.tableName},#{item.score},NOW())
            </foreach>
    </insert>

</mapper>
