<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcy.datamanage.biz.mapper.DataAssessFrequencyMapper">

    <insert id="batchReplace" parameterType="com.dcy.datamanage.api.model.DataAssessFrequency">
        REPLACE INTO data_assess_frequency (id,data_source_id,`table_name`,frequency_unit)
        VALUE
            <foreach collection="list" item="item" separator=",">
                (#{item.id},#{item.dataSourceId},#{item.tableName},#{item.frequencyUnit})
            </foreach>
    </insert>
</mapper>
