<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ygkj.big.screen.mapper.HystStatisticalMapper">


    <select id="selectRealTimeData" resultType="com.ygkj.big.screen.model.HystStatistical">
        select * from ${tableName}
        <where>
            <if test="startDate != null">
                and sta_time &gt; #{startDate}
            </if>
            <if test="endDate != null">
                and sta_time &lt; #{endDate}
            </if>
            <if test="true">
                and hyst_code is not null
            </if>
            <if test="hystCode != null and hystCode != ''">
                and hyst_code = #{hystCode}
            </if>
        </where>
        order by sta_time
    </select>



</mapper>