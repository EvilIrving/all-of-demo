<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ygkj.big.screen.mapper.BsInfluentRelMapper">

    <resultMap id="BaseResultMap" type="com.ygkj.big.screen.model.BsInfluentRel">
        <result column="id" property="id"/>
        <result column="group_data_id" property="groupDataId"/>
        <result column="inf_group_data_id" property="infGroupDataId"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,
        group_data_id,
        inf_group_data_id
    </sql>

    <insert id="insert" parameterType="com.ygkj.big.screen.model.BsInfluentRel">
        INSERT INTO lc_meta_db.bs_influent_rel
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test='null != id'>
                id,
            </if>
            <if test='null != groupDataId'>
                group_data_id,
            </if>
            <if test='null != infGroupDataId'>
                inf_group_data_id
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test='null != id'>
                #{id},
            </if>
            <if test='null != groupDataId'>
                #{groupDataId},
            </if>
            <if test='null != infGroupDataId'>
                #{infGroupDataId}
            </if>
        </trim>
    </insert>

    <delete id="delete">
        DELETE FROM bs_influent_rel
        WHERE id = #{id}
    </delete>

    <update id="update" parameterType="com.ygkj.big.screen.model.BsInfluentRel">
        UPDATE bs_influent_rel
        <set>
            <if test='null != groupDataId'>group_data_id = #{groupDataId},</if>
            <if test='null != infGroupDataId'>inf_group_data_id = #{infGroupDataId}</if>
        </set>
        WHERE id = #{id}
    </update>


    <select id="load" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List"/>
        FROM bs_influent_rel
        WHERE id = #{id}
    </select>

    <select id="pageList" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List"/>
        FROM bs_influent_rel
        LIMIT #{offset}, #{pageSize}
    </select>

    <select id="pageListCount" resultType="java.lang.Integer">
        SELECT count(1)
        FROM bs_influent_rel
    </select>


</mapper>