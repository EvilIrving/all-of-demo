<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ygkj.water.project.mapper.EmergencySupportMapper">


    <select id="candidateWareHouse" resultType="com.ygkj.flood.model.AttWarehouseBase">
        SELECT * FROM att_warehouse_base
        WHERE id IN
        <foreach collection="codes" separator="," open="(" close=")" item="item">
            #{item}
        </foreach>
        and del_flag = 0
    </select>


    <select id="candidateTeam" resultType="com.ygkj.flood.model.AttTeamBase">
        SELECT * FROM att_team_base
        WHERE id IN
        <foreach collection="codes" separator="," open="(" item="item" close=")">
            #{item}
        </foreach>
        AND del_flag = 0
    </select>

</mapper>