<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ygkj.water.project.mapper.WaterPlantMapper">
    <insert id="insertBatch" parameterType="com.ygkj.project.model.StWaterPlantR">
        REPLACE INTO st_water_plant_r(water_plant_code,water_plant_name,time,dpd,turbidity,ph)
        VALUE
        <foreach collection="list" item="item" separator=",">
            (#{item.waterPlantCode},#{item.waterPlantName},#{item.time}, #{item.dpd},#{item.turbidity},#{item.ph})
        </foreach>
    </insert>
</mapper>