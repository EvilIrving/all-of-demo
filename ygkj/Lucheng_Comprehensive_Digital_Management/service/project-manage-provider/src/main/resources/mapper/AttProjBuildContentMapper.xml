<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ygkj.water.project.mapper.AttProjBuildContentMapper">

    <sql id="Base_Column_List">
        id,
        proj_code,
        name,
        type,
        url,
        created_by,
        created_time,
        updated_by,
        updated_time,
        del_flag
    </sql>

    <select id="getProBuildContentsByProC" resultType="com.ygkj.project.model.AttProjBuildContent">
        select <include refid="Base_Column_List" />
        from att_proj_build_content where del_flag = 0
        <if test="projCode != null and projCode != ''">
            and proj_code = #{projCode}
        </if>
        <if test="type != null and type != ''">
            and `type` = #{type}
        </if>
    </select>
</mapper>