<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ygkj.mapper.SysArchiveLabelMapper">

    <resultMap id="BaseResultMap" type="com.ygkj.auth.api.archiveLabel.model.SysArchiveLabel" >
        <result column="id" property="id" />
        <result column="name" property="name" />
        <result column="type" property="type" />
        <result column="create_time" property="createTime" />
        <result column="creator_id" property="creatorId" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <sql id="Base_Column_List">
                id,
                name,
                type,
                create_time,
                creator_id,
                update_time,
                del_flag
    </sql>

    <insert id="insert" useGeneratedKeys="true" keyColumn="id" keyProperty="id" parameterType="com.ygkj.auth.api.archiveLabel.model.SysArchiveLabel">
        INSERT INTO sys_archive_label
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test ='null != name'>
                name,
            </if>
            <if test ='null != type'>
                type,
            </if>
            <if test ='null != createTime'>
                create_time,
            </if>
            <if test ='null != creatorId'>
                creator_id,
            </if>
            <if test ='null != updateTime'>
                update_time,
            </if>
            <if test ='null != delFlag'>
                del_flag
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test ='null != name'>
                #{name},
            </if>
            <if test ='null != type'>
                #{type},
            </if>
            <if test ='null != createTime'>
                #{createTime},
            </if>
            <if test ='null != creatorId'>
                #{creatorId},
            </if>
            <if test ='null != updateTime'>
                #{updateTime},
            </if>
            <if test ='null != delFlag'>
                #{delFlag}
            </if>
        </trim>
    </insert>

    <update id="delete" >
        UPDATE sys_archive_label set del_flag =1
        WHERE id = #{id}
    </update>

    <update id="update" parameterType="com.ygkj.auth.api.archiveLabel.model.SysArchiveLabel">
        UPDATE sys_archive_label
        <set>
            <if test ='null != name'>name = #{name},</if>
            <if test ='null != type'>type = #{type},</if>
            <if test ='null != createTime'>create_time = #{createTime},</if>
            <if test ='null != creatorId'>creator_id = #{creatorId},</if>
            <if test ='null != updateTime'>update_time = #{updateTime},</if>
            <if test ='null != delFlag'>del_flag = #{delFlag}</if>
        </set>
        WHERE id = #{id}
    </update>


    <select id="load" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List" />
        FROM sys_archive_label
        WHERE id = #{id}
    </select>

    <select id="pageList" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List" />
        FROM sys_archive_label
        LIMIT #{offset}, #{pageSize}
    </select>

    <select id="pageListCount" resultType="java.lang.Integer">
        SELECT count(1)
        FROM sys_archive_label
    </select>
    <select id="selectAll" resultType="com.ygkj.auth.api.archiveLabel.model.SysArchiveLabel">
        select
        <include refid="Base_Column_List"></include>
        from sys_archive_label where del_flag = 0
        <if test="name!=null and name!=''">
            and name like concat('', #{name}, '')
        </if>
    </select>

</mapper>