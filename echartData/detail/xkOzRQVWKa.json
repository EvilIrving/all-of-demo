.length; i < len; i++) {\r\n        if (i < dayCount) {\r\n            result.push('-');\r\n            continue;\r\n        }\r\n        var sum = 0;\r\n        for (var j = 0; j < dayCount; j++) {\r\n            sum += data0.values[i - j][1];\r\n        }\r\n        result.push(sum / dayCount);\r\n    }\r\n    return result;\r\n}\r\n\r\n\r\n//计算标准偏差\r\nfunction stddev(data) {\r\n    //var data = [1, 2, 3, 4, 5];\r\n    var sum = function(x, y) {\r\n        return x + y;\r\n    };\r\n    var square = function(x) {\r\n        return x * x;\r\n    };\r\n    var mean = data.reduce(sum) / data.length;\r\n    var deviations = data.map(function(x) {\r\n        return x - mean;\r\n    });\r\n    var stddeva = Math.sqrt(deviations.map(square).reduce(sum) / (data.length));\r\n    return stddeva;\r\n}\r\n\r\n//计算布林轨\r\nfunction calculatebull(dayCount) {\r\n    var result = [];\r\n    var result2 = [];\r\n    for (var i = 0, len = data0.values.length; i < len; i++) {\r\n        if (i < dayCount) {\r\n            result.push('-');\r\n            result2.push('-');\r\n            continue;\r\n        }\r\n        var temp = [];\r\n        var sum = 0;\r\n        for (var j = 0; j < dayCount; j++) {\r\n            sum += data0.values[i - j][1];\r\n            temp.push(data0.values[i - j][1]);\r\n\r\n        }\r\n        var sum2 = sum / dayCount;\r\n        var stddeva = stddev(temp);\r\n        var b = sum2 + stddeva * 2;\r\n        var a = sum2 - stddeva * 2;\r\n        // var ab=[];\r\n        //ab.push(a);\r\n        //ab.push(b);\r\n        result2.push(b);\r\n        result.push(a);\r\n\r\n    }\r\n    return {\r\n        result2: result2,\r\n        result: result\r\n    };\r\n}\r\n\r\nvar bull = calculatebull(20);\r\noption = {\r\n    title: {\r\n        text: '上证指数',\r\n        left: 0\r\n    },\r\n    tooltip: {\r\n        trigger: 'axis',\r\n        axisPointer: {\r\n            type: 'line'\r\n        }\r\n    },\r\n    legend: {\r\n        data: ['日K', 'MA5', 'MA10