',\n        sublink: 'https://www.macrofocus.com/public/products/infoscope/datasets/pricesandearnings/',\n        left: 'center',\n        top: 5,\n        itemGap: 0,\n        textStyle: {\n            color: '#eee',\n        },\n    },\n    tooltip: {\n        trigger: 'axis',\n    },\n    geo: {\n        map: 'china',\n        silent: true,\n        roam: true,\n        zoom: 1,\n        center: [116.46, 39.92],\n        label: {\n            emphasis: {\n                show: false,\n                areaColor: '#eee',\n            },\n        },\n        itemStyle: {\n            normal: {\n                borderWidth: 0.2,\n                borderColor: '#404a59',\n            },\n        },\n        itemStyle: {\n            normal: {\n                areaColor: '#323c48',\n                borderColor: '#111',\n            },\n            emphasis: {\n                areaColor: '#2a333d',\n            },\n        },\n    },\n    series: [],\n};\n\nfunction renderEachCity() {\n    var option = { xAxis: [], yAxis: [], grid: [], series: [] };\n    var inflationStartIdx = 14;\n    var inflationYearCount = 6;\n    var inflationYearStart = '2006';\n    var xAxisCategory = [];\n    for (var i = 0; i < inflationYearCount; i++) {\n        xAxisCategory.push(+inflationYearStart + i + '');\n    }\nObject.keys(rawData).forEach(function (dataItem, idx) {\n    \n    var geoCoord = geoCoordMap[rawData[idx][0]];\n        var coord = myChart.convertToPixel('geo', geoCoord);\n        idx += '';\n        // console.log(coord)\n\n        inflationData = [];\n        for (var k = 0; k < inflationYearCount; k++) {\n            inflationData.push(rawData[idx][inflationStartIdx + k]);\n        }\n        // console.log(inflationData)\n\n        option.xAxis.push({\n            id: idx,\n            gridId: idx,\n            type: 'category',\n            name: rawData[idx][0],\n            nameStyle: {\n                color: '#ddd',\n                fontSize: 12,\n            },\n            nameLocation: 'middle',\n            nameGap: 3,\n            splitLine: { show: false },\n            axisTick: { show: false },\n            axisLabel: { show: false },\n            axisLine: {\n                onZero: false,\n                lineStyle: {\n                    color: '#bbb',\n                },\n            },\n            data: xAxisCategory,\n            z: 100,\n        });\n        option.yAxis.push({\n            id: idx,\n            gridId: idx,\n            splitLine: { show: false },\n            axisTick: { show: false },\n            axisLabel: { show: false },\n            axisLine: {\n                lineStyle: {\n                    color: '#bbb',\n                },\n            },\n            z: 100,\n        });\n        option.grid.push({\n            id: idx,\n            width: 30,\n            height: 30,\n            left: coord[0] - 15,\n            top: coord[1] - 15,\n            z: 100,\n        });\n        option.series.push({\n            id: idx,\n            type: 'line',\n            xAxisId: idx,\n            yAxisId: idx,\n            data: inflationData,\n            z: 100,\n        });\n})\n\n\n/* 使用\n *Object.keys(rawData).forEach(function (dataItem, idx) {})\n *替换\n *echarts.util.each(rawData, function (dataItem, idx) {})\n */\n    // echarts.util.each(rawData, function (dataItem, idx) {\n    //     var geoCoord = geoCoordMap[dataItem[0]];\n    //     var coord = myChart.convertToPixel('geo', geoCoord);\n    //     idx += '';\n    //     // console.log(coord)\n\n    //     inflationData = [];\n    //     for (var k = 0; k < inflationYearCount; k++) {\n    //         inflationData.push(dataItem[inflationStartIdx + k]);\n    //     }\n\n    //     option.xAxis.push({\n    //         id: idx,\n    //         gridId: idx,\n    //         type: 'category',\n    //         name: dataItem[0],\n    //         nameStyle: {\n    //             color: '#ddd',\n    //             fontSize: 12,\n    //         },